<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Sutro.Slicer</PackageId>
    <Version>0.0.0</Version>
    <PackageDescription>Library for additive manufacturing.</PackageDescription>
    <RepositoryUrl>https://github.com/SutroMachine/Sutro.Slicer</RepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <PropertyGroup>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>
      $(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb
    </AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>gs</RootNamespace>
    <Authors>Ryan Schmidt</Authors>
    <Company>gradientspace</Company>
    <Copyright>Copyright © Ryan Schmidt/gradientspace 2017</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Library for additive manufacturing.</Description>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="ReportGenerator" Version="4.5.0" />
    <PackageReference Include="Sutro.Clipper" Version="1.0.0" />
    <PackageReference Include="Sutro.PathWorks.Plugins.API" Version="2.0.0" />
    <PackageReference Include="System.ComponentModel.Composition" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\gsGCode\gsGCode\gsGCode.csproj" />
  </ItemGroup>

</Project>
